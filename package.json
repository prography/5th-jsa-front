{
  "name": "5th-jsa-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "immer": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "react": "^16.11.0",
    "react-dnd": "^9.4.0",
    "react-dnd-html5-backend": "^9.4.0",
    "react-dom": "^16.11.0",
    "react-infinite-scroller": "^1.2.4",
    "react-kakao-login": "^1.2.0",
    "react-redux": "^7.1.0-rc.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-scroll": "^1.7.15",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "simple-react-dnd": "^0.3.2",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "cp .env.production .env && cross-env NODE_PATH=src react-scripts start",
    "start:dev": "cp .env.development .env && cross-env NODE_ENV=development NODE_PATH=src react-scripts start",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "import/no-unresolved": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/prop-types": 0,
      "react/no-array-index-key": 0,
      "react-hooks/exhaustive-deps": 0,
      "no-underscore-dangle": 0,
      "no-empty": 0,
      "no-console": 1,
      "import/prefer-dafualt-export": 0
    },
    "env": {
      "browser": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
